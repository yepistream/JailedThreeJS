{
  "name": "jailedthreejs",
  "version": "0.9.2-beta.1",
  "description": "Dedicated To Willson :D, A small, fast and lightweight Three.js framework.",
  "license": "MIT",
  "author": "M.K.",
  "type": "module",
  "main": "dist/lib/index.js",
